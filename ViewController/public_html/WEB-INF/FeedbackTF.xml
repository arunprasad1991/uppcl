<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="FeedbackTF">
    <default-activity>feedbackForm</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>FeedbackForm</managed-bean-name>
      <managed-bean-class>com.xx.uppcl.beans.Feedback</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="feedbackForm">
      <page>/WEB-INF/fragments/feedback.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
